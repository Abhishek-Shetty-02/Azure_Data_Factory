{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shettyadf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"ls_AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_AzureSqlDatabase'"
		},
		"ls_AzureSqlDatabase2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_AzureSqlDatabase2'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://abhishekshettysa.dfs.core.windows.net/"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "shettyserver.database.windows.net"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Shettydb"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "shetty"
		},
		"ls_AzureSqlDatabase2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "shettyserver.database.windows.net"
		},
		"ls_AzureSqlDatabase2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Shettydb"
		},
		"ls_AzureSqlDatabase2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "shetty"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "shettyadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"ls_AzureSqlDatabase_password": {
						"value": "[parameters('ls_AzureSqlDatabase_password')]"
					},
					"ls_AzureSqlDatabase2_password": {
						"value": "[parameters('ls_AzureSqlDatabase2_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_database')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_userName')]"
					},
					"ls_AzureSqlDatabase2_properties_typeProperties_server": {
						"value": "[parameters('ls_AzureSqlDatabase2_properties_typeProperties_server')]"
					},
					"ls_AzureSqlDatabase2_properties_typeProperties_database": {
						"value": "[parameters('ls_AzureSqlDatabase2_properties_typeProperties_database')]"
					},
					"ls_AzureSqlDatabase2_properties_typeProperties_userName": {
						"value": "[parameters('ls_AzureSqlDatabase2_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}